  "definitions": {
    "SchemaType": {
      "description": "possible schema types that indicate the type of schema.",
      "enum": [
        "any",
        "any-object",
        "array",
        "binary",
        "boolean",
        "byte-array",
        "char",
        "choice",
        "conditional",
        "constant",
        "credential",
        "date",
        "date-time",
