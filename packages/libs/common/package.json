{
  "name": "@autorest/common",
  "version": "1.5.2",
  "description": "Autorest common utilities",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "watch": "tsc -p tsconfig.build.json --watch",
    "build": "tsc -p tsconfig.build.json",
    "lint:fix": "eslint ./src --fix --ext .ts",
    "lint": "eslint ./src --ext .ts --max-warnings=0",
    "prepare": "npm run build",
    "test": "jest --watchAll --coverage=false",
    "test:ci": "jest --ci",
    "clean": "rimraf ./dist ./temp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/autorest.git"
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/autorest/issues"
  },
  "homepage": "https://github.com/Azure/autorest/tree/main/packages/libs/common",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "~14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "@types/cli-progress": "~3.9.2",
    "eslint-plugin-jest": "~24.3.2",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~3.4.0",
    "eslint-plugin-unicorn": "~33.0.1",
    "eslint-plugin-import": "~2.24.2",
    "eslint": "^7.17.0",
    "jest": "^28.0.0-alpha.7",
    "rimraf": "^3.0.2",
    "typescript": "~4.4.2"
  },
  "dependencies": {
    "@azure-tools/datastore": "~4.6.1",
    "@azure-tools/yaml": "~1.1.0",
    "@azure-tools/json": "~1.2.2",
    "@azure/logger": "^1.0.2",
    "chalk": "^4.1.0",
    "cli-progress": "~3.10.0"
  }
}
