  "main": "./dist/main.js",
  "typings": "./dist/main.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/autorest.git"
  },
  "scripts": {
    "watch": "tsc -p tsconfig.build.json --watch",
    "build": "tsc -p tsconfig.build.json",
